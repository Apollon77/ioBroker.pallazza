{
    "common": {
        "name": "pallazza",
        "version": "0.1.0",
        "news": {
          "0.1.0": {
            "en": "First functional version",
            "de": "Erste funktionale Version"
          },
          "0.0.1": {
              "en": "initial version",
              "de": "Initiale Version"
          }
        },
		"title": "HSP 6 PALLAZZA-III 534.08 adapter",
        "titleLang": {
            "en": "HSP 6 PALLAZZA-III 534.08 adapter",
            "de": "HSP 6 PALLAZZA-III 534.08 Adapter"
        },
        "desc": {
            "en": "Enables controlling a HSP 6 PALLAZZA-III 534.08 adapter",
            "de": "Erm√∂glicht die Steuerung eines HSP 6 PALLAZZA-III 534.08 adapter"
        },
        "authors": [
            "Marvin Grieger <mail@marvingrieger.de>"
        ],
        "docs": {
            "en": "docs/en/admin.md",
            "de": "docs/de/admin.md"
        },
        "platform": "Javascript/Node.js",
        "mode": "once",
        "icon": "haassohn.png",
        "materialize": true,
        "enabled": true,
        "extIcon": "https://raw.githubusercontent.com/grieger/ioBroker.pallazza/master/admin/haassohn.png",
        "keywords": ["haas", "sohn", "fireplace", "home automation"],
        "readme": "https://github.com/grieger/ioBroker.pallazza/blob/master/README.md",
        "loglevel": "info",
        "type": "general",
        "restartAdapters": ["vis"],
        "license": "MIT"
    },
    "native": {
        "fireplaceAddress": "127.0.0.1:4200",
        "pollingInterval": "10",
        "pin": "0000",
        "supportedHwSwVersions": "{\"KS01_V6.02\":true}"
    },
    "instanceObjects": [
        {
            "_id": "device",
            "type": "channel",
            "common":
            {
                "name": "Device information"
            },
            "native": {}
        },
        {
          "_id": "device.meta",
          "type": "channel",
          "common": {
            "name": "Meta information"
          },
          "native": {}
        },
        {
          "_id": "connected",
          "type": "state",
          "common": {
            "role": "indicator.connected",
            "name": "Connection to device successful?",
            "type": "boolean",
            "read": true
          },
          "native": {}
        },
        {
          "_id": "missing_state",
          "type": "state",
          "common": {
            "name": "Is a state missing?",
            "type": "boolean",
            "read": true
          },
          "native": {}
        },
        {
          "_id": "device.meta.sw_version",
          "type": "state",
          "common": {
            "name": "Software version",
            "type": "string",
            "read": true,
            "write": false
          },
          "native": {}
        },
        {
          "_id": "device.meta.hw_version",
          "type": "state",
          "common": {
            "name": "Hardware version",
            "type": "string",
            "read": true,
            "write": false
          },
          "native": {}
        },
        {
          "_id": "device.meta.bootl_version",
          "type": "state",
          "common": {
            "name": "Bootloader version",
            "type": "string",
            "read": true,
            "write": false
          },
          "native": {}
        },
        {
          "_id": "device.meta.wifi_sw_version",
          "type": "state",
          "common": {
            "name": "Wifi firmware version",
            "type": "string",
            "read": true,
            "write": false
          },
          "native": {}
        },
        {
          "_id": "device.meta.wifi_bootl_version",
          "type": "state",
          "common": {
            "name": "Wifi bootloader version",
            "type": "string",
            "read": true,
            "write": false
          },
          "native": {}
        },
        {
          "_id": "device.meta.sn",
          "type": "state",
          "common": {
            "name": "Serial Number",
            "type": "string",
            "read": true,
            "write": false
          },
          "native": {}
        },
        {
          "_id": "device.meta.typ",
          "type": "state",
          "common": {
            "name": "Device type",
            "type": "string",
            "read": true,
            "write": false
          },
          "native": {}
        },
        {
          "_id": "device.meta.language",
          "type": "state",
          "common": {
            "name": "Language",
            "type": "string",
            "read": true,
            "write": false
          },
          "native": {}
        },
        {
          "_id": "device.meta.nonce",
          "type": "state",
          "common": {
            "name": "Current nonce",
            "type": "string",
            "read": true,
            "write": false
          },
          "native": {}
        },
        {
          "_id": "device.meta.wlan_features",
          "type": "state",
          "common": {
            "name": "Wifi features",
            "type": "string",
            "read": true,
            "write": false
          },
          "native": {}
        },
        {
            "_id": "device.prg",
            "type": "state",
            "common": {
              "name": "Is the oven active?",
              "type": "boolean",
              "read": true,
              "write": true
            },
            "native": {}
        },
        {
          "_id": "device.wprg",
          "type": "state",
          "common": {
            "name": "Is the weekprogram active?",
            "type": "boolean",
            "read": true,
            "write": false
          },
          "native": {}
        },
        {
          "_id": "device.mode",
          "type": "state",
          "common": {
            "name": "Mode of operation",
            "type": "string",
            "read": true,
            "write": false,
            "states": {
              "off": "off",
              "start": "start",
              "heating": "heating"
            }
          },
          "native": {}
        },
        {
          "_id": "device.sp_temp",
          "type": "state",
          "common": {
            "name": "Target temperature",
