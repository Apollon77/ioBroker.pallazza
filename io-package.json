            "name": "Week program",
            "type": "string",
            "read": true,
            "write": false
          },
          "native": {}
        },
        {
          "_id": "device.error",
          "type": "state",
          "common": {
            "name": "Error",
            "type": "string",
            "read": true,
            "write": false
          },
          "native": {}
        },
        {
          "_id": "device.eco_mode",
          "type": "state",
          "common": {
            "name": "Eco mode indicator",
            "type": "boolean",
            "read": true,
            "write": false
          },
          "native": {}
        },
        {
          "_id": "device.pgi",
          "type": "state",
          "common": {
            "name": "?",
            "type": "boolean",
            "read": true,
            "write": false
          },
          "native": {}
        },
        {
          "_id": "device.ignitions",
          "type": "state",
          "common": {
            "name": "Number of ignitions",
            "type": "number",
            "read": true,
            "write": false
          },
          "native": {}
        },
        {
          "_id": "device.on_time",
          "type": "state",
          "common": {
            "name": "?",
            "type": "number",
            "read": true,
            "write": false
          },
          "native": {}
        },
        {
          "_id": "device.consumption",
          "type": "state",
          "common": {
            "name": "Consumed pellets",
            "type": "number",
            "read": true,
            "write": false,
            "unit": "kg"
          },
          "native": {}
        },
        {
          "_id": "device.maintenance_in",
          "type": "state",
          "common": {
            "name": "Duration until maintenance required",
            "type": "number",
            "read": true,
            "write": false,
            "unit": "minutes"
          },
          "native": {}
        },
        {
          "_id": "device.cleaning_in",
